import java.util.*;

class Main{
    public static void main(String[] args) {
        Stack s=new Stack();
        s.push(3);
        s.push(2);
        s.push(8);
        s.push(9);
       s.display();
    }
} 
    class LinkList {
        int val;
        LinkList next;

        LinkList(int d) {
            val = d;
            next = null;
        }
    }

     class Stack {
      LinkList head;

        public boolean isEmpty() {
            return head == null;
        }

        public  void push(int d) {
            LinkList n = new LinkList(d);
            if (isEmpty()) {
                head = n;
            } else {
                n.next = head;
                head = n;
            }
        }

        public  int pop() {
            if (isEmpty()) {
                return -1;
            } else {
                int top = head.val;
                head = head.next;
                return top;
            }
        }

        public  int peek() {
            if (isEmpty()) {
                return -1;
            }
            return head.val;
        }
        
        public void display(){
           while (!isEmpty()) {
            System.out.println(peek());
            pop();
        }
        } 
    }
