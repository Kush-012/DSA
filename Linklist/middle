class Solution {
    public ListNode middleNode(ListNode head) {
        ListNode temp=head;
        int c=0;
        while(temp!=null){
            c++;
            temp=temp.next;
        } 
        int mid=(c/2)+1;
        int var=0;
        while(var<mid-1 && head!=null){
            head=head.next;
            var++;
        }
        return head;
    }
} 
