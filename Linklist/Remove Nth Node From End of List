class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        ListNode temp=head;
        int c=0;
        while(temp!=null){
            c++;
            temp=temp.next;
        } 
        int x=c-n;
        if(n==c){
            return head.next;
        } 
        temp=head;
        while(x>1){
             temp=temp.next;
            x--;
        }
        
        
        temp.next=temp.next.next;
        return head;
    }
} 
