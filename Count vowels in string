import java.util.*;

public class Main {
    public static void main(String[] args) {
      String s="hello";
      int v=0,c=0;
      char arr[]=s.toCharArray();
      for(int i=0;i<arr.length;i++){
        if(arr[i]=='a' || arr[i]=='e' || arr[i]=='i'|| arr[i]=='o' || arr[i]=='u'){
          v++;
        } 
        else{
          c++;
        } 
      }
      System.out.print("Vowels: "+v);
  }
}
